{
  "manifest_version": 2,

  "name": "Fb Alert",
  "description": "This extension alerts you when u are using fb for more time.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.jpg",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  
 
  "content_scripts": [
    {
      "matches": ["https://www.facebook.com/"],
   
      "js": ["myScript.js"]
    }
  ]
}